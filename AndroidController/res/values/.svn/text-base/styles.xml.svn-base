<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    
    <style name="empty_theme"></style>
    
    <!-- 公共提示框 -->
	<style name="Dialog" parent="android:style/Theme.Dialog">
		<item name="android:windowFrame">@null</item><!--边框 -->
		<item name="android:windowIsFloating">true</item><!--是否浮现在activity之上 -->
		<item name="android:windowIsTranslucent">true</item><!--半透明 -->
		<item name="android:windowNoTitle">true</item><!--无标题 -->
		<item name="android:windowBackground">@android:color/transparent</item><!--背景透明 -->
		<item name="android:backgroundDimEnabled">true</item><!--模糊 -->
	</style>
	
	<!-- 公共提示框文本 -->
	<style name="DialogText">
		<item name="android:textColor">#FFFFFF</item>
		<item name="android:textSize">14sp</item>
		<item name="android:includeFontPadding">false</item>
	</style>
	
	<!-- 公共提示框标题 -->
	<style name="DialogText.Title">
		<item name="android:textSize">15sp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:textColor">#F5740A</item>
		<item name="android:includeFontPadding">false</item>
	</style>
	
	<!-- ListItem中的主标题样式 -->
	<style name="BigBlackTitle">
		<item name="android:textColor">#3d3d3d</item>
		<item name="android:textSize">18sp</item>
		<item name="android:textStyle">bold</item>
	</style>
	
	<!-- 正在缓存字样 -->
	<style name="CachingText">
		<item name="android:textColor">#998b71</item>
		<item name="android:textSize">16sp</item>
		<item name="android:textStyle">bold</item>
	</style>
	
	<!-- 单曲列表中播放模式显示字体 -->
	<style name="PlayModeText">
		<item name="android:textColor">#615347</item>
		<item name="android:textSize">20sp</item>
		<item name="android:textStyle">bold</item>
	</style>
	
	<!-- ListItem中的主标题样式 -->
	<style name="TabTitle">
		<item name="android:textColor">#fff</item>
		<item name="android:textSize">23sp</item>
	</style>
	
	<!-- 自定义搜索框Edit  -->
	<style name="EtSearch"  parent="android:style/Widget.EditText">
        <item name="android:focusable">true</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/et_search</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMediumInverse</item>
        <item name="android:textColor">?android:attr/editTextColor</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    
	<!-- 暂无数据字体样式  -->
	<style name="NoDataText" >
		<item name="android:textColor">@color/text_color_on_cream</item>
		<item name="android:textSize">25sp</item>
		<item name="android:textStyle">italic</item>
    </style>
    
	    
    <!-- 清除搜索记录字体样式 -->
    <style name="ClearSearchHistoryTextStyle" parent="android:style/Widget.EditText">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/text_color_on_cream</item>
        <item name="android:textSize">@dimen/clear_search_history_textsize</item>
        <item name="android:textStyle">bold</item>
    </style>
    
	<!-- spinner 动画 -->
	   <style name="PopupAnimation" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/popup_enter_anim</item>
        <item name="android:windowExitAnimation">@anim/popup_exit_anim</item>
    </style>

	<style name="login_hint_style">
	    <item name="android:textColor">@color/login_hint_color</item>
	    <item name="android:textSize">30sp</item>
	</style>
	<style name="login_device_item_style">
	    <item name="android:textColor">@color/login_list_item_color</item>
	    <item name="android:textSize">35sp</item>
	    <item name="android:paddingLeft">70dp</item>
	</style>
	
		<!-- mymusic -->
	<style name="ExampleTheme" parent="Theme.Sherlock.Light.DarkActionBar">
            <item name="homeAsUpIndicator">@drawable/new_indicator</item>
            <item name="android:homeAsUpIndicator">@drawable/new_indicator</item>
	</style>
</resources>
